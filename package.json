{
	"name": "warehouse",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"android": "react-native run-android --mode=devDebug",
		"android:dev": "react-native run-android --mode=devDebug",
		"android:qa": "react-native run-android --mode=qaDebug",
		"android:prod": "react-native run-android --mode=prodDebug",
		"ios": "rm -rf node_modules && rm -rf ios/build && npm i && cd ios && pod install && react-native run-ios --scheme \"devDebug\"",
		"ios:dev": "rm -rf node_modules && rm -rf ios/build && npm i && cd ios && pod install && react-native run-ios --scheme \"devDebug\"",
		"ios:qa": "rm -rf node_modules && rm -rf ios/build && npm i && cd ios && pod install && react-native run-ios --scheme \"qaDebug\"",
		"ios:prod": "rm -rf node_modules && rm -rf ios/build && npm i && cd ios && pod install && react-native run-ios --scheme \"prodDebug\"",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier *.{js,ts,tsx} --write",
		"start": "react-native start",
		"start:qa": "react-native start",
		"start:prod": "react-native start",
		"test": "jest",
		"bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
		"build:android": "npm run bundle:android && cd android && ./gradlew assembleDevDebug",
		"build:android:dev": "npm run bundle:android && cd android && ./gradlew assembleDevDebug",
		"build:android:apk:dev": "npm run bundle:android && cd android && ./gradlew assembleDevDebug",
		"build:android:aab:dev": "cd android && rm -rf app/build && ./gradlew bundleDev",
		"build:android:qa": "npm run bundle:android && cd android && rm -rf app/build && ./gradlew assembleQaDebug",
		"build:android:apk:qa": "npm run bundle:android && cd android && rm -rf app/build && ./gradlew assembleQaDebug",
		"build:android:aab:qa": "cd android && rm -rf app/build && ./gradlew bundleQa",
		"build:android:prod": "cd android && rm -rf app/build && ./gradlew bundleProd",
		"build:android:apk:prod": "npm run bundle:android && cd android && rm -rf app/build && ./gradlew assembleProdDebug",
		"build:android:aab:prod": "cd android && rm -rf app/build && ./gradlew bundleProd",
		"version:major": "npm version major --no-git-tag-version && react-native-version --never-amend",
		"version:minor": "npm version minor --no-git-tag-version && react-native-version --never-amend",
		"version:patch": "npm version patch --no-git-tag-version && react-native-version --never-amend",
		"version:premajor": "npm version premajor --no-git-tag-version && react-native-version --never-amend",
		"version:preminor": "npm version preminor --no-git-tag-version && react-native-version --never-amend",
		"version:prepatch": "npm version prepatch --no-git-tag-version && react-native-version --never-amend",
		"version:prerelease": "npm version prerelease --no-git-tag-version && react-native-version --never-amend",
		"version:from-git": "npm version from-git && react-native-version --never-amend",
		"react-native-version": "react-native-version --never-amend",
		"uninstall:android": "adb uninstall com.warehouse",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{ts,js,tsx}": [
			"npm run lint -- --fix",
			"npm run format"
		]
	},
	"dependencies": {
		"@react-native-async-storage/async-storage": "^1.23.1",
		"@react-navigation/bottom-tabs": "^6.5.18",
		"@react-navigation/native": "^6.1.10",
		"@react-navigation/stack": "^6.3.21",
		"@reduxjs/toolkit": "^2.2.1",
		"@tanstack/react-query": "^5.28.9",
		"axios": "^1.6.7",
		"moment": "^2.30.1",
		"react": "18.2.0",
		"react-native": "^0.73.4",
		"react-native-app-auth": "^7.1.3",
		"react-native-appstore-version-checker": "^3.0.0",
		"react-native-config": "^1.5.1",
		"react-native-device-info": "^10.13.2",
		"react-native-gesture-handler": "^2.15.0",
		"react-native-safe-area-context": "^4.9.0",
		"react-native-screens": "^3.29.0",
		"react-native-size-matters": "^0.4.2",
		"react-native-splash-screen": "^3.3.0",
		"react-native-storage": "^1.0.1",
		"react-native-uuid": "^2.0.2",
		"react-native-version": "^4.0.0",
		"react-redux": "^9.1.0",
		"yup": "^1.4.0"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@babel/preset-env": "^7.20.0",
		"@babel/runtime": "^7.20.0",
		"@react-native/babel-preset": "0.73.21",
		"@react-native/eslint-config": "0.73.2",
		"@react-native/metro-config": "0.73.5",
		"@react-native/typescript-config": "^0.73.1",
		"@types/react": "^18.2.6",
		"@types/react-test-renderer": "^18.0.0",
		"babel-jest": "^29.6.3",
		"babel-plugin-module-resolver": "^5.0.2",
		"eslint": "^8.57.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-detox": "^1.0.0",
		"husky": "^9.0.11",
		"jest": "^29.6.3",
		"lint-staged": "^15.2.5",
		"prettier": "^2.8.8",
		"react-test-renderer": "18.2.0",
		"typescript": "5.0.4"
	},
	"engines": {
		"node": ">=18"
	}
}
